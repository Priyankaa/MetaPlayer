<?xml version="1.0"?><controls>	<region id="tpAdCountdownRegion">		<row id="tpAdCountdownContainer">			<control id="tpAdCountdown"/>		</row>	</region>	<region id="tpBottomRegion" alpha="75">		<row>			<control id="tpPlay"/>			<control id="tpNext"/>			<group percentWidth="100" direction="horizontal" verticalAlign="middle">				<control id="tpScrubber"/>			</group>			<group direction="horizontal" verticalAlign="middle">				<spacer width="15"/>				<control id="tpCurrentTime"/>				<control id="tpTimeDivider"/>				<control id="tpTotalTime"/>				<spacer width="15"/>			</group>			<control id="tpVolume" expandDirection="up"/>			<control id="tpCC"/>			<control id="tpBandwidth"/>			<control id="tpFullScreen"/>			<control id="tpMenu"/>		</row>	</region></controls>